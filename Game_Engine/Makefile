ifeq ($(config),debug)
  RESCOMP = windres
  TARGETDIR = Build/Debug-windows-x86_64/PrEngine
  TARGET = $(TARGETDIR)/PrEngine.lib
  OBJDIR = Build/Obj/Debug-windows-x86_64/PrEngine
  DEFINES +=
  INCLUDES += -Iinclude -IVendor -IVendor/include -IVendor/imgui-master -IVendor/imgui-master/backends
  FORCE_INCLUDE +=
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m32
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m32 -std=c++17 -s USE_SDL=2 -s USE_SDL_MIXER=2
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += 
  LDDEPS +=
  ALL_LDFLAGS += $(LDFLAGS) -L../vendor/libs -L/usr/lib64 -m32 -s USE_WEBGL2=1 -s FULL_ES3=1 -s ALLOW_MEMORY_GROWTH=1 -s 
  LINKCMD = $(AR) -rcs "$@" $(OBJECTS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: prebuild prelink $(TARGET)
	@:

endif

